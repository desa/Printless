<% provide :title, @user.name %>
<div class="content">
  <div id="topcontent">
    <div class="profileinfo">
    <h1 id="name"> <%= @user.name %> </h1>

      <div class="profilepict" id="michael_desa_picture">
      <%= image_tag @user.image_url %>
      </div>

      <div class="about">
       <div class="aboutword">About</div> <p><%= @user.about_me %> </p>
      </div>

    <%= render '/shared/stats' %>
    <%= render 'follow_form' if signed_in? %>
    </div>
    <%= render 'users/my_projects' %>
  </div>
    <%= render 'users/my_articles' %>
</div>